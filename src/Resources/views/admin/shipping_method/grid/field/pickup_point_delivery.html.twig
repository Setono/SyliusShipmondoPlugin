{# @var data \Setono\SyliusShipmondoPlugin\Model\ShippingMethodInterface #}
{% if data.pickupPointDelivery %}
    <span class="ui basic label">
        <i class="checkmark icon"></i>
        {{ 'sylius.ui.yes_label'|trans }} ({{ data.carrierCode }})
    </span>
{% else %}
    <span class="ui basic label">
        <i class="remove icon"></i>
        {{ 'sylius.ui.no_label'|trans }}
    </span>
{% endif %}
